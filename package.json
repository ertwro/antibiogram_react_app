{
  "name": "antibiogram_react_app",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:bacteria && npm run build:syndromes && npm run build:antibiotics && npm run build:clsi && vite build",
    "build:bacteria": "node scripts/generate-import-index.js",
    "build:syndromes": "node scripts/generate-syndromes-index.js",
    "build:antibiotics": "node scripts/generate-antibiotics-index.js",
    "build:clsi": "node scripts/generate-clsi-index.js",
    "build:data": "npm run build:bacteria && npm run build:syndromes && npm run build:antibiotics && npm run build:clsi",
    "sync-data": "node scripts/sync-json-data.js",
    "generate-imports": "node scripts/generate-import-index.js",
    "watch:syndromes": "node scripts/generate-syndromes-index.js --watch",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "vite": "^6.3.5"
  }
}

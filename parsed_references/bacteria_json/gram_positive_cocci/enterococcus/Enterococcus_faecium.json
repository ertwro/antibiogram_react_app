      "keySignsAndSymptoms": [
        "Fever",
        "Symptoms specific to the site of infection"
      ],
      "clinicalSyndromes": [
        {
          "syndromeName": "Bacteremia",
          "description": "A common presentation, often in critically ill patients."
        },
        {
          "syndromeName": "Endocarditis",
          "description": "A serious infection of the heart valves."
        },
        {
          "syndromeName": "Urinary Tract Infection (UTI)",
          "description": "Can cause both cystitis and pyelonephritis."
        }
      ],
      "stagesOfIllness": [],
      "highRiskPopulations": [
        "Hospitalized patients",
        "Immunocompromised patients",
        "Patients with long-term antibiotic exposure"
      ],

      "antimicrobialStewardship": [
        "Daptomycin dose > 9 mg/kg is associated with lower mortality in treatment of bacteremia due to VRE.",
        "Linezolid should be used for treatment of VRE infections if the Daptomycin MIC > 4 µg/mL."
      ],
      "drugSpecificPearls": [
        {
          "drugName": "Daptomycin",
          "pearl": "Best to use daptomycin in combination therapy. Concomitant beta-lactam impedes development of resistance and can reverse resistance if daptomycin resistance is already evident."
        },
        {
          "drugName": "Oritavancin",
          "pearl": "Oritavancin has in vitro activity against vancomycin-resistant enterococci but its role in therapy is not well defined."
        }
      ],

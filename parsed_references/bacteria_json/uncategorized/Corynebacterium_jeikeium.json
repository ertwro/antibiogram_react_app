    "clinicalProfile": {
      "summary": "Corynebacterium jeikeium is a multi-drug resistant, opportunistic pathogen primarily causing hospital-acquired infections in immunocompromised patients. It is a significant cause of bacteremia, endocarditis, and infections related to intravascular devices and prosthetics.",
      "pathophysiologyPearls": [
        "Its lipophilic nature allows it to colonize the skin of hospitalized patients.",
        "Adherence to prosthetic materials is a key step in the pathogenesis of device-related infections."
      ],
      "keySignsAndSymptoms": [
        "Fever",
        "Signs of sepsis",
        "Erythema or purulence at catheter exit site",
        "New or changing heart murmur (endocarditis)"
      ],
      "clinicalSyndromes": [
        {
          "syndromeName": "Bacteremia",
          "description": "Often associated with central venous catheters."
        },
        {
          "syndromeName": "Endocarditis",
          "description": "Can occur on native or prosthetic valves."
        },
        {
          "syndromeName": "Prosthetic Device Infection",
          "description": "Infections of prosthetic joints, heart valves, or vascular grafts."
        },
        {
          "syndromeName": "CSF Shunt Infection",
          "description": "Infection of cerebrospinal fluid shunts."
        }
      ],
      "highRiskPopulations": [
        "Immunocompromised patients (e.g., hematologic malignancy, AIDS)",
        "Neutropenic patients",
        "Patients with long-term indwelling catheters or prosthetic devices",
        "Patients with prolonged hospital stays and prior antibiotic exposure"
      ],
      "transmissionVectors": [
        "Nosocomial transmission, likely from patient's own skin flora or hands of healthcare workers."
      ],
